{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/home/index",
      "style": {
        "navigationStyle": "custom",
        "backgroundColorTop": "#FFFFFF",
        "backgroundColorBottom": "#FFFFFF"
      }
    },
    {
      "path": "pages/personal/index",
      "style": {
        "navigationBarTitleText": "personal",
        "navigationStyle": "custom",
        "backgroundColorTop": "#0087ff",
        "backgroundColorBottom": "#FFFFFF"
      }
    },
    {
      "path": "components/webview/index"
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F8F8F8",
    "usingComponents": {
      "privacy": "/wxcomponents/privacy/privacy"
    }
  },
  "tabBar": {
    "selectedColor": "#0084FF",
    "backgroundColor": "#FFFFFF",
    "color": "#333333",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/home/index",
        "iconPath": "static/tabbar/home_gray.png",
        "selectedIconPath": "static/tabbar/home.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/personal/index",
        "iconPath": "static/tabbar/person_gray.png",
        "selectedIconPath": "static/tabbar/person.png",
        "text": "我的"
      }
    ]
  },
  "subPackages": [
    {
      "root": "subPackagesA",
      "pages": [
        {
          "path": "personal/privacyPolicy",
          "style": {
            "navigationBarTitleText": "隐私政策"
          }
        },
        {
          "path": "personal/userAgreement",
          "style": {
            "navigationBarTitleText": "用户协议"
          }
        },
        {
          "path": "personal/chooseLoginType",
          "style": {
            "navigationBarTitleText": "登录说客英语",
            "disableScroll": true
          }
        },
        {
          "path": "personal/phoneLogin",
          "style": {
            "navigationBarTitleText": "手机登录",
            "disableScroll": true
          }
        }
      ]
    }
  ]
}
